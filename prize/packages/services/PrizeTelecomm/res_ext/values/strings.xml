    <!-- voice recorder -->
    <!-- Phone call record: all the records will be put in a media player playlist
         naming "My recordings" -->
    <string name="str_db_playlist_name">My recordings</string>
    <!-- Phone call record error message: toast if recording stopped due to audio device error -->
    <string name="alert_device_error">Error occurred on audio device</string>
    <!-- Phone call record error message: toast if recording stopped due to SD removed -->
    <string name="ext_media_badremoval_notification_title">SD card unexpectedly removed</string>
    <!-- Phone call record error message: toast if recording stopped due to accessing SD failed -->
    <string name="error_sdcard_access">Unable to access SD card</string>
    <!-- Phone call record error message: toast if recording stopped due to SD full.
         In this case, the current recording has already saved successfully -->
    <string name="confirm_device_info_full">SD card is out of space, the recording has been saved.</string>
    <!-- Phone call record error message: toast if recorded file was saved to somewhere -->
    <string name="confirm_save_info_saved_to">The recording has been saved to</string>

    <!-- volte ims call -->
    <!-- A waring dialog telling user the IMS was disabled in Settings.
         This dialog will popup when user trying to make a IMS(VoLTE) call.
         This string is the dialog's title -->
    <string name="reminder">Reminder</string>
    <!-- A waring dialog telling user the IMS was disabled in Settings.
         This dialog will popup when user trying to make a IMS(VoLTE) call.
         This string is the dialog's content.
         This dialog has 2 options, yes and no. If user press "yes", will launch Settings
         for user to enable the IMS. -->
    <string name="enable_ims_dialog_message">IMS call is disabled, would you like to enable it?</string>

    <!-- Error toast: A general error prompt to user that failed to dial out the call -->
    <string name="outgoing_call_failed">Call not sent.</string>
    <!-- Toast message after a call ended. will combine with call_duration_format -->
    <string name="call_duration_title">Call ended</string>
    <!-- Toast message after a call ended. will combine with call_duration_title -->
    <string name="call_duration_format">"<xliff:g id="HOURS">%1$s</xliff:g> hours <xliff:g id="MINUTES">%2$s</xliff:g> minutes <xliff:g id="SECONDS">%3$s</xliff:g> seconds"</string>

    <!-- Add for [VoLTE_SS] -->
    <!-- An error dialog telling user something wrong happens. The "sub_name" means the user
