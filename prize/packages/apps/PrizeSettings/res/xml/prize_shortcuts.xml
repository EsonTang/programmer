<shortcuts xmlns:android="http://schemas.android.com/apk/res/android" >
    <shortcut
        android:shortcutId="prize_data"
        android:icon="@drawable/prize_shortcuts_mobile_data"
        android:shortcutShortLabel="@string/prize_mobile_data">
        <intent
            android:action="android.intent.action.MAIN"
            android:targetPackage="com.android.settings"
            android:targetClass="com.android.settings.Settings$SimSettingsActivity" />
    </shortcut>

    <shortcut
        android:shortcutId="prize_wifi"
        android:icon="@drawable/prize_shortcuts_wifi"
        android:shortcutShortLabel="@string/wifi_settings">
        <intent
            android:action="android.intent.action.MAIN"
            android:targetPackage="com.android.settings"
            android:targetClass="com.android.settings.Settings$WifiSettingsActivity" />
    </shortcut>
    <shortcut
        android:shortcutId="prize_blutooth"
        android:icon="@drawable/prize_shortcuts_blutooth"
        android:shortcutShortLabel="@string/bluetooth">
        <intent
            android:action="android.intent.action.MAIN"
            android:targetPackage="com.android.settings"
            android:targetClass="com.android.settings.Settings$BluetoothSettingsActivity" />
    </shortcut>
    <shortcut
        android:shortcutId="prize_system_update"
        android:icon="@drawable/prize_shortcuts_system_update"
        android:shortcutShortLabel="@string/system_update_settings_title">
        <intent
            android:action="android.intent.action.MAIN"
            android:targetPackage="com.adups.fota"
            android:targetClass="com.adups.fota.GoogleOtaClient" />
    </shortcut>

</shortcuts>