<!--M: add for [Multi-Delete] -->
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginEnd="15dp"
    android:background="@color/prize_dialer_multiple_top_bg_color"
    android:orientation="horizontal">

    <!--<Button
        android:id="@+id/delete"
        style="?android:attr/actionButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_centerVertical="true"
        android:layout_gravity="center_vertical"
        android:layout_marginEnd="10dip"
        android:background="@drawable/mtk_ic_calllog_delete"
        android:padding="10dip"
        android:singleLine="true"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/dialtacts_primary_text_color"
        android:textSize="12sp"
        android:textStyle="bold"/>-->
    <TextView
        android:id="@+id/prize_cancel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="0dp"
        android:gravity="center"
        android:singleLine="true"
        android:text="@string/prize_cancel_string"
        android:textColor="@drawable/prize_dialer_multiple_tv_selector"
        android:textSize="13sp"/>

    <!-- add for CallLogMultipleChoiceActivity. Start -->
    <Button
        android:id="@+id/confirm"
        style="?android:attr/actionButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_centerVertical="true"
        android:layout_marginEnd="14dp"
        android:gravity="center"
        android:singleLine="true"
        android:text="@android:string/ok"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/dialtacts_primary_text_color"
        android:textSize="12sp"
        android:textStyle="bold"
        android:visibility="gone"/>

    <ImageView
        android:id="@+id/ic_divider2"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="center_vertical"
        android:layout_marginBottom="10dip"
        android:layout_marginEnd="2dip"
        android:layout_marginStart="2dip"
        android:layout_marginTop="10dip"
        android:alpha="0.9"
        android:gravity="center"
        android:scaleType="fitXY"
        android:src="@drawable/mtk_divider_vertical_dark"
        android:tint="@color/actionbar_icon_color"
        android:visibility="gone"/>
    <!-- add for CallLogMultipleChoiceActivity. End -->

    <LinearLayout
        android:id="@+id/prize_select_items_layout"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_centerInParent="true"
        android:orientation="horizontal">

        <Button
            android:id="@+id/select_items"
            style="?android:attr/actionButtonStyle"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:divider="?android:attr/listDividerAlertDialog"
            android:drawablePadding="-10dip"
            android:ellipsize="none"
            android:gravity="left|center"
            android:requiresFadingEdge="horizontal"
            android:singleLine="true"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="@android:color/black"
            android:textSize="16sp"
            android:textStyle="bold"/>
    </LinearLayout>

    <TextView
        android:id="@+id/prize_select_all_or_not"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_centerVertical="true"
        android:layout_marginEnd="0dp"
        android:gravity="center"
        android:textColor="@drawable/prize_dialer_multiple_tv_selector"
        android:textSize="13sp"/>

    <!-- <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_alignParentBottom="true"
        android:background="#c8c8c8" /> -->
</RelativeLayout>
