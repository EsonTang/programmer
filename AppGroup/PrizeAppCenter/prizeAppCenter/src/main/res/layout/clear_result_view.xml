<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
				android:layout_width="match_parent"
				android:layout_height="match_parent" >
	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="vertical" >

		<RelativeLayout
			android:layout_width="match_parent"
			android:layout_height="55dp"
			android:layout_marginLeft="15dp"
			android:layout_marginRight="15dp"
			android:gravity="center_vertical">

			<ImageView
				android:id="@+id/check_view"
				android:layout_alignParentRight="true"
				android:layout_centerVertical="true"
				android:paddingLeft="10dp"
				android:paddingTop="10dp"
				android:paddingBottom="10dp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content" />

			<RelativeLayout
				android:id="@+id/safe_rl_view"
				android:layout_toLeftOf="@id/check_view"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:gravity="center_vertical">

				<TextView
					android:id="@+id/safe_clear"
					android:layout_alignParentLeft="true"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_centerVertical="true"
					android:text="@string/clear_sdk_safe_clear"
					android:textSize="15sp"
					android:textColor="#2b2b2b"/>

				<TextView
					android:id="@+id/safe_size"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_alignParentRight="true"
					android:layout_centerVertical="true"
					android:textSize="15sp"
					android:textColor="#2b2b2b" />
			</RelativeLayout>
		</RelativeLayout>

		<View
			android:layout_width="match_parent"
			android:layout_height="1px"
			android:layout_marginLeft="15dp"
			android:layout_marginRight="15dp"
			android:background="#d7d7d7" />

		<RelativeLayout
			android:id="@+id/weixin_rl"
			android:layout_width="fill_parent"
			android:layout_height="55dp"
			android:layout_marginLeft="15dp"
			android:layout_marginRight="15dp"
			android:gravity="center_vertical" >

			<TextView
				android:id="@+id/weixin_trash"
				android:layout_alignParentLeft="true"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_centerVertical="true"
				android:text="@string/clear_sdk_weixin_clear"
				android:textSize="15sp"
				android:textColor="#2b2b2b" />

			<ImageView
				android:id="@+id/weixin_arrow_iv"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:src="@drawable/node_not_expanded"
				android:layout_toRightOf="@id/weixin_trash"
				android:layout_marginLeft="11dp"
				android:layout_centerVertical="true" />

			<TextView
				android:id="@+id/weixin_size"
				android:layout_alignParentRight="true"
				android:layout_centerVertical="true"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:textSize="15sp"
				android:textColor="#2b2b2b" />
		</RelativeLayout>

		<LinearLayout
			android:id="@+id/weixin_ll"
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			android:visibility="gone" >
		</LinearLayout>


		<View
			android:id="@+id/divider_d7d7d7_wx"
			android:layout_width="match_parent"
			android:layout_height="1px"
			android:layout_marginLeft="15dp"
			android:layout_marginRight="15dp"
			android:background="#d7d7d7" />

		<RelativeLayout
			android:id="@+id/qq_rl"
			android:layout_width="fill_parent"
			android:layout_height="55dp"
			android:layout_marginLeft="15dp"
			android:layout_marginRight="15dp"
			android:gravity="center_vertical" >

			<TextView
				android:id="@+id/qq_trash"
				android:layout_alignParentLeft="true"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="@string/clear_sdk_qq_clear"
				android:layout_centerVertical="true"
				android:textSize="15sp"
				android:textColor="#2b2b2b" />

			<ImageView
				android:id="@+id/qq_arrow_iv"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:src="@drawable/node_not_expanded"
				android:layout_toRightOf="@id/qq_trash"
				android:layout_marginLeft="11dp"
				android:layout_centerVertical="true" />

			<TextView
				android:id="@+id/qq_size"
				android:layout_alignParentRight="true"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_centerVertical="true"
				android:textSize="15sp"
				android:textColor="#2b2b2b" />
		</RelativeLayout>

		<LinearLayout
			android:id="@+id/qq_ll"
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			android:visibility="gone" >
		</LinearLayout>

		<View
			android:id="@+id/divider_d7d7d7_qq"
			android:layout_width="match_parent"
			android:layout_height="1px"
			android:layout_marginLeft="15dp"
			android:layout_marginRight="15dp"
			android:background="#d7d7d7" />

		<LinearLayout
			android:id="@+id/tree_container"
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:orientation="vertical" />
	</LinearLayout>
</RelativeLayout>