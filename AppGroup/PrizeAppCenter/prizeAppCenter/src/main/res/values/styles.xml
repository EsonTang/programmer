    <style name="cloud_phone_num_style">
        <item name="android:layout_marginLeft">14dp</item>
        <item name="android:background">#f5f5f5</item>
        <item name="android:inputType">number</item>
        <item name="android:maxLength">11</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/text_color_6c6c6c</item>
        <item name="android:textColorHint">@color/prize_text_default</item>
        <item name="android:textSize">@dimen/text_size_30</item>
    </style>

    <style name="cloud_login_password_style">
        <item name="android:drawableLeft">@drawable/phone_password</item>
        <item name="android:drawablePadding">14dp</item>
        <item name="android:inputType">textPassword</item>
        <item name="android:maxLength">16</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/text_color_6c6c6c</item>
        <item name="android:textColorHint">@color/prize_text_default</item>
        <item name="android:textSize">@dimen/text_size_30</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">28dp</item>
        <item name="android:paddingRight">25dp</item>
    </style>

    <style name="TreeNodeStyle">
        <item name="android:paddingLeft">20dp</item>
    </style>


