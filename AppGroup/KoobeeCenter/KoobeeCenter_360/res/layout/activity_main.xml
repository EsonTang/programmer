<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    android:orientation="vertical" >

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/activity_main_one"
        android:background="@drawable/bg" />

    <View
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/activity_main_two"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:background="#e0e0e0"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/activity_main_four"
            android:layout_margin="0.5dp" >

            <RelativeLayout
                android:id="@+id/system_update_Rlyt"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/relative_cln_selector"
                android:clickable="true" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="7.5dp"
                    android:clickable="false"
                    android:focusable="false"
                    android:gravity="center_vertical"
                    android:text="@string/system_update"
                    android:textColor="#323232"
                    android:textSize="@dimen/text_size_small" >
                </TextView>

                <View
                    android:layout_width="20dp"
                    android:layout_height="@dimen/activity_main_six"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="9.5dp"
                    android:background="@drawable/right_point"
                    android:clickable="false"
                    android:focusable="false" >
                </View>
            </RelativeLayout>

            <View
                android:layout_width="0.5dp"
                android:layout_height="match_parent"
                android:background="#e0e0e0" >
            </View>

            <RelativeLayout
                android:id="@+id/after_servicee_Rlyt"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/relative_cln_selector"
                android:clickable="true" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="7.5dp"
                    android:clickable="false"
                    android:focusable="false"
                    android:gravity="center_vertical"
                    android:text="@string/sale_service"
                    android:textColor="#323232"
                    android:textSize="@dimen/text_size_small" >
                </TextView>

                <View
                    android:layout_width="20dp"
                    android:layout_height="@dimen/activity_main_six"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="9.5dp"
                    android:background="@drawable/right_point"
                    android:clickable="false"
                    android:focusable="false" >
                </View>
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/activity_main_four"
            android:layout_marginBottom="0.5dp"
            android:layout_marginLeft="0.5dp"
            android:layout_marginRight="0.5dp" >

            <RelativeLayout
                android:id="@+id/feedback_relative"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/relative_cln_selector"
                android:clickable="true" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="7.5dp"
                    android:clickable="false"
                    android:focusable="false"
                    android:gravity="center_vertical"
                    android:text="@string/feedback"
                    android:textColor="#323232"
                    android:textSize="@dimen/text_size_small" >
                </TextView>

                <View
                    android:layout_width="20dp"
                    android:layout_height="@dimen/activity_main_six"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="9.5dp"
                    android:background="@drawable/right_point"
                    android:clickable="false"
                    android:focusable="false" >
                </View>
            </RelativeLayout>

            <View
                android:layout_width="0.5dp"
                android:layout_height="match_parent"
                android:background="#e0e0e0" >
            </View>

            <RelativeLayout
                android:id="@+id/tmall_Rlyt"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/relative_cln_selector"
                android:clickable="true" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="7.5dp"
                    android:clickable="false"
                    android:focusable="false"
                    android:gravity="center_vertical"
                    android:text="@string/flag_store"
                    android:textColor="#323232"
                    android:textSize="@dimen/text_size_small" >
                </TextView>

                <View
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="9.5dp"
                    android:background="@drawable/right_point"
                    android:clickable="false"
                    android:focusable="false" >
                </View>
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/activity_main_four"
            android:layout_marginBottom="0.5dp"
            android:layout_marginLeft="0.5dp"
            android:layout_marginRight="0.5dp" >

            <RelativeLayout
                android:id="@+id/koobee_Rlyt"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/relative_cln_selector"
                android:clickable="true" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="7.5dp"
                    android:clickable="false"
                    android:focusable="false"
                    android:gravity="center_vertical"
                    android:text="@string/visit_koobee"
                    android:textColor="#323232"
                    android:textSize="@dimen/text_size_small" />

                <View
                    android:layout_width="20dp"
                    android:layout_height="@dimen/activity_main_six"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="9.5dp"
                    android:background="@drawable/right_point"
                    android:clickable="false"
                    android:focusable="false" >
                </View>
            </RelativeLayout>
        </LinearLayout>
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:background="#e0e0e0" />

    <View
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:gravity="center_vertical"
        android:text="@string/company_name"
        android:textColor="#323232"
        android:textSize="@dimen/text_size_small" >
    </TextView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/activity_main_nine"
        android:layout_marginLeft="@dimen/activity_main_eight"
        android:layout_marginTop="1dp"
        android:gravity="center_vertical"
        android:text="@string/koobee_official_website_title"
        android:textColor="#555555"
        android:textSize="12sp" />

</LinearLayout>