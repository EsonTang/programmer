<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
     android:background="@color/color_f5f5f5"
    android:clipToPadding="false"
    android:fitsSystemWindows="true"
    android:orientation="vertical">

    <include
        layout="@layout/titlebar"
        android:layout_width="match_parent"
        android:layout_height="@dimen/title_h"
      
         />
       <ImageView 
            android:id="@+id/divide_line"
            android:layout_width="match_parent"
            android:layout_height="2.5dp"
            android:layout_marginTop="0dp"
             android:src="@drawable/top_ying"
            />
   
        
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="#f5f5f5"
        android:layout_marginTop="0dp"
        >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/row_h"
            android:background="@drawable/row_slt"
            android:onClick="toOutlets_clk"
           
           
            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/alloutlets"
                android:textColor="@color/color_323232"
                android:textSize="@dimen/textsize_15"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="20dp"
                 />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="18.5dp"
                android:layout_centerVertical="true"
                android:src="@drawable/right_point" />

        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/common_line_height"
            android:layout_marginLeft="@dimen/layout_margin"
            android:layout_marginRight="@dimen/layout_margin"
            android:background="@color/color_e0e0e0" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/row_h"
            android:background="@drawable/row_slt"
            android:onClick="toHotline_clk"
            
           
            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"

                android:text="@string/hotline"
                android:textColor="@color/color_323232"
                android:textSize="@dimen/textsize_15"
                 android:layout_alignParentLeft="true"
                android:layout_marginLeft="20dp"
                 />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                
                android:layout_marginRight="18.5dp"
                android:layout_centerVertical="true"
                android:src="@drawable/right_point" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/common_line_height"
            android:layout_marginLeft="@dimen/layout_margin"
            android:layout_marginRight="@dimen/layout_margin"
            android:background="@color/color_e0e0e0" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/row_h"
            android:background="@drawable/row_slt"
            android:onClick="toIntro_clk"
            
            
            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/aftersales_instroduction"
                android:textColor="@color/color_323232"
                android:textSize="@dimen/textsize_15" 
                 android:layout_alignParentLeft="true"
                android:layout_marginLeft="20dp"
                />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="18.5dp"
                android:layout_centerVertical="true"
                android:src="@drawable/right_point" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/common_line_height"
            android:layout_marginLeft="@dimen/layout_margin"
            android:layout_marginRight="@dimen/layout_margin"
            android:background="@color/color_e0e0e0" />
    </LinearLayout>

</LinearLayout>
